# === Aplicação ===
APP_NAME=NARA Diagnostic API
APP_VERSION=1.0.0
DEBUG=false
ENV=development

# === Supabase (obrigatórias) ===
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_role_key
ADMIN_API_KEY=your_admin_api_key
ADMIN_USERNAME=admin
ADMIN_PASSWORD=change_me_admin_password
ADMIN_JWT_SECRET=change_me_admin_jwt_secret
ADMIN_TOKEN_TTL_MINUTES=60

# === OpenAI (obrigatória) ===
OPENAI_API_KEY=your_openai_api_key

# === Modelos OpenAI (opcionais, têm default) ===
OPENAI_MODEL_QUESTIONS=gpt-4o-mini
OPENAI_MODEL_ANALYSIS=gpt-4o
OPENAI_EMBEDDING_MODEL=text-embedding-3-small

# === Resend (obrigatória para envio de emails) ===
RESEND_API_KEY=your_resend_api_key
EMAIL_FROM=diagnostic.nara@phellipeoliveira.org
RESEND_REPLY_TO=contato@phellipeoliveira.org

# === Frontend (para links em emails) ===
# Local: http://localhost:5173
# Produção (Vercel): https://naraproject-beige.vercel.app (sem barra no final)
FRONTEND_URL=http://localhost:5173

# === RAG (opcionais) ===
RAG_TOP_K=10
RAG_SIMILARITY_THRESHOLD=0.7

# === Diagnóstico (opcionais) ===
MIN_QUESTIONS_TO_FINISH=40
MIN_WORDS_TO_FINISH=3500
QUESTIONS_PER_PHASE=15
MIN_AREAS_COVERED=12

# === CORS (lista JSON ou separada por vírgula) ===
# Exemplo JSON: CORS_ORIGINS='["http://localhost:5173","https://nara.app"]'
CORS_ORIGINS=["http://localhost:5173","http://localhost:5174","https://nara.app"]

